    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/adam7/delugia-code/releases/download/v$version/Delugia.Nerd.Font.Complete.ttf"
    },
    "installer": {
        "script": [
            "$AccessRule = New-Object System.Security.AccessControl.FileSystemAccessRule(\"NT AUTHORITY\\LOCAL SERVICE\", \"ReadAndExecute, Synchronize\", \"ContainerInherit, ObjectInherit\", \"None\", \"Allow\")",
            "$Acl = Get-Acl $dir",
            "$Acl.SetAccessRule($AccessRule)",
            "Set-Acl -Path $dir -AclObject $Acl",
            "$TargetKey = \"HKCU:\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Fonts\\Scoop.Delugia-Nerd-Font-Complete\"",
            "New-Item -Path $TargetKey -Force | Out-Null",
            "Get-ChildItem \"$dir/\" -Filter '*.ttf' | ForEach-Object {",
            "    New-ItemProperty -Path $TargetKey -Name $_.Name -Value $_.FullName -Force | Out-Null",
            "}"
        ]
    },
    "uninstaller": {
        "script": [
            "Remove-Item -Path \"HKCU:\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Fonts\\Scoop.Delugia-Nerd-Font-Complete\""
        ]
    }
}