    "installer": {
        "script": [
            "New-Item -Path 'HKCU:\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Fonts\\Scoop.AurulentSansMono' -Force | Out-Null",
            "Get-ChildItem $dir -filter '*Windows Compatible.*' | ForEach-Object {",
